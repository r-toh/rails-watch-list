<h1>Movie Lists</h1>
<div class="container  d-flex p-2 bd-highlight">
  <% @lists.each do |list| %>
    <div class="card" style="width: 18rem;">
      <div class="card-body">
        <h5 class="card-title"><%= link_to list.name, list_path(list) %></h5>
      </div>
    </div>
    <% end %>
</div>

<%= link_to "Add new list", new_list_path %>
